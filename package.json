{
  "name": "csv-to-line-chart",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "d3": "^5.7.0",
    "localforage": "^1.7.3",
    "prop-types": "^15.6.2",
    "randomcolor": "^0.5.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.1.0",
    "react-scripts": "2.0.5",
    "redux": "^4.0.1",
    "redux-saga": "^0.16.2",
    "styled-components": "^4.0.2",
    "styled-reset": "^1.6.0",
    "underscore": "^1.9.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "quotes": [
        "error",
        "single",
        {
          "avoidEscape": true
        }
      ],
      "indent": [
        "error",
        2
      ],
      "semi": [
        "error",
        "always"
      ],
      "comma-dangle": [
        "error",
        {
          "arrays": "always-multiline",
          "objects": "always-multiline",
          "imports": "always-multiline",
          "exports": "always-multiline",
          "functions": "always-multiline"
        }
      ]
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
